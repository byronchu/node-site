

  <%- contentFor('hero') %>
  <section class="hero">
    <div class="hero-content">
      <h2>All About us</h2>
      <p>We are a growing team</p>


      <a href="/contact" class="btn btn-lg btn-primary">find out more</a>
    </div>
  </section>

  <%- contentFor('body') %>
  <div class="jumbotron text-center">
    <h1>The Team</h1>
    <div class="row">
     <% for (user of users) { %>
       <div class="col-sm-3">
         <h2> <%= user.name %> </h2>
         <img src="<%= user.avatar %> " alt="" class="img-responsive">

       </div>


    <% } %>

    </div>
  </div>
